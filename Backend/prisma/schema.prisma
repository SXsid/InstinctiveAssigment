
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}



model Student{
  id             String @id @default(uuid())
  name           String
  cohort         String
  courses        Json
  joined_date    DateTime  @default(now())
  last_login     DateTime   @default(now())
  status         Boolean    @default(false)
}    